					"when": { "active": "false" },
					"apply": { "model": "betterpets:block/water_filter_seperator" }
				}
			]
        },
        {
            "when": { "facing": "east" },
            "apply": [
				{ "model": "betterpets:block/granite_water_filter", "y": 90 },
				{
					"when": { "active": "false" },
					"apply": { "model": "betterpets:block/water_filter_seperator", "y": 90 }
				}
			]
        },
        {
            "when": { "facing": "south" },
            "apply": [
				{ "model": "betterpets:block/granite_water_filter", "y": 180 },
				{
					"when": { "active": "false" },
					"apply": { "model": "betterpets:block/water_filter_seperator", "y": 180 }
				}
			]
        },
        {
            "when": { "facing": "west" },
            "apply": [
				{ "model": "betterpets:block/granite_water_filter", "y": 270 },
				{
					"when": { "active": "false" },
					"apply": { "model": "betterpets:block/water_filter_seperator", "y": 270 }
